{
  "name": "protractor-to-webdriver-demo",
  "version": "1.0.0",
  "description": "Project to demonstrate converting a protractor e2e testing project to use selenium webdriver",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf temp/ && rimraf download/ && rimraf test-results/",
    "pretest": "npm run clean && npm run tsc",
    "test": "protractor temp/conf.js",
    "webdriver-update": "node node_modules/protractor/bin/webdriver-manager update"
  },
  "keywords": [
    "Protractor",
    "Selenium WebDriver",
    "protractor",
    "jasmine",
    "typescript",
    "javascript",
    "angular",
    "angularjs",
    "vscode",
    "testing",
    "selenium",
    "webdriverJS",
    "automation testing"
  ],
  "author": "Timmy F. Richard",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.21",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "jasmine": "^4.0.2",
    "protractor": "^7.0.0",
    "protractor-beautiful-reporter": "^1.3.6",
    "typescript": "^4.6.2",
    "webdriver-manager": "^12.1.8"
  }
}
